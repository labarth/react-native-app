{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {
      "cwd": "babelrc",
      "root": ["./src"],
      "alias": {
        "@containers": "MyApp/src/containers"
      },
      "extensions": [".js", ".ios.js", ".android.js", ".json"]
    }]
  ]
}